func main()
	var primes: []int :: #[2000000]int
	do primes[0] :: 2
	do primes[1] :: 3
	var primesNum: int :: 2
	var sum: int :: 5
	for num(5, 2000000, 2)
		for i(0, primesNum - 1)
			if(num % primes[i] = 0)
				skip num
			end if
		end for
		do primes[primesNum] :: num
		do sum :+ num
		do primesNum :+ 1
	end for
	do cui@print("\{sum}\n")
end func
