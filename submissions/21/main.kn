func main()
	func d(n: int): int
		var res: int :: 1
		for i(2, n)
			if(n % i = 0)
				var j: int :: n / i
				do res :+ i
				do res :+ j <> i ?(j, 0)
			end if
			if(i * i >= n)
				ret res
			end if
		end for
	end func
	
	var ans: int :: 0
	for i(1, 10000)
		var t: int :: d(i)
		if(t < i & d(t) = i)
			do ans :+ i + t
		end if
	end for
	do cui@print("\{ans}\n")
end func
